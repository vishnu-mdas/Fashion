        <section class="mt-50 mb-50">
            <div class="container">
               <% if (cartItems.items.length > 0) { %>

                <div class="row">
                    <div class="col-12">
                        <div class="table-responsive">
                            
                             
                                <table class="table shopping-summery text-center clean">
                                    <thead>
                                        <tr class="main-heading">
                                            <th scope="col">Image</th>
                                            <th scope="col">Name</th>
                                            <th scope="col">Price</th>
                                            <th scope="col">Quantity</th>
                                            <th scope="col">Subtotal</th>
                                            <th scope="col">Remove</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% cartItems.items.forEach(cart=> { %>

                                                <tr data-prodId="<%= cart.productid._id %>">
                                                <td class="image product-thumbnail"><img
                                                        src="/productImages/<%=cart.productid.image[0] %>" alt="#"></td>
                                                <td class="product-des product-name">
                                                    <h5 class="product-name"><a href="shop-product-right.html">
                                                            <%=cart.productid.productTitle %>
                                                        </a></h5>
                                                    <p class="font-xs">
                                                        <%=cart.productid.description %> <br>
                                                    </p>
                                                </td>
                                                <td class="price" data-title="Price"><span>
                                                        ₹ <%=cart.productid.offerPrice %>
                                                    </span></td>
                                                <td class="text-center" data-title="Stock">
    
                                                    <button class="cart-item-count ml-4"
                                                        id="decrement-button<%=cart.productid._id%> qtyBtn"
                                                        onclick="changeQuantity('<%=userId%>','<%=cart.productid._id%>',-1,'<%=cart.productid.offerPrice%>')">
                                                        -
                                                    </button>
                                                    <span class="qty-val" id="<%=cart.productid._id%>">
                                                        <%=cart.quantity %>
                                                    </span>
                                                    <button class="cart-item-count mr-4" id="increment-button qtyBtn"
                                                        onclick="changeQuantity('<%=userId%>','<%=cart.productid._id%>',1,'<%=cart.productid.offerPrice%>')">
                                                        +
                                                    </button>
    
    
                                                    <!--  -->
                                                </td>
    
                                                <td class="text-right" data-title="Cart">
                                                    <span class="subTotal" id="subtotal<%=cart.productid._id%>">
                                                        ₹ <%= cart.productid.offerPrice * cart.quantity %>
                                                    </span>
                                                </td>
                                                <td class="action" data-title="Remove"><a
                                                    onclick="removefromcart('<%=cart.productid._id %>')"
                                                      class="text-muted"><i
                                                            class="fi-rs-trash"></i></a></td>
                                            </tr>
    
                                        <% }) %>
    
                                                
                                                <tr>
                                                    <td colspan="6" class="text-end">
                                                        <!-- <a href="#" class="text-muted"> <i class="fi-rs-cross-small"></i> Clear Cart</a> -->
                                                    </td>
                                                </tr>
                                    </tbody>
                                </table>


                           
                        </div>
                        <div class="cart-action text-end">
                            <!-- <a class="btn  mr-10 mb-sm-15"><i class="fi-rs-shuffle mr-10"></i>Update Cart</a> -->
                            <a href="/" class="btn "> <i class="fi-rs-shopping-bag mr-10"></i>Continue Shopping</a>
                </div>
                 <div class="divider center_icon mt-50 mb-50"><i class="fi-rs-fingerprint"></i></div>
                        <div class="row mb-50">
                            <div class="col-lg-6 col-md-12">
                                <div class="heading_s1 mb-3">
                                    <h4>Free Shipping</h4>
                                </div>
                                <p class="mt-15 mb-30">Across all Over India <span
                                        class="font-xl text-brand fw-900"></span></p>
                                <form class="field_form shipping_calculator">


                                    <div class="form-row">
                                        <!-- <div class="form-group col-lg-12">
                                            <button class="btn  btn-sm"><i class="fi-rs-shuffle mr-10"></i>Update</button>
                                        </div> -->
                                    </div>
                                </form>
                                <div class="mb-30 mt-50">
                                    <div class="heading_s1 mb-3">
                                        <h4>Apply Coupon</h4>
                                    </div>
                                    <div class="total-amount">
                                        <div class="left">
                                            <div class="coupon">
                                                <form action="#" target="_blank">
                                                    <div class="form-row row justify-content-center">
                                                        <div class="form-group col-lg-6">
                                                            <input class="font-medium" name="Coupon"
                                                                placeholder="Enter Your Coupon"
                                                                id="couponcode"
                                                                />
                                                        </div>
