                <div class="card-body">
                    <% products.forEach(i => { %>   
                    <article class="itemlist">
                        <div class="row align-items-center">
                            <div class="col col-check flex-grow-0">
                                <div class="form-check">
                                    <input class="form-check-input" type="checkbox">
                                </div>
                            </div>
                            <div class="col-lg-4 col-sm-4 col-8 flex-grow-1 col-name">
                                <a class="itemside" href="#">
                                    <div class="left">
                                        <img src="/productImages/<%= i.image[0] %>" class="img-sm img-thumbnail" alt="Item">
                                    </div>
                                    <div class="info">
                                        <h6 class="mb-0"><%= i.productTitle  %></h6>
                                    </div>
                                </a>
                            </div>
       
                            <div class="col-lg-2 col-sm-2 col-4 col-price"> <span>â‚¹ <%=i.offerPrice  %></span> </div>
                            <div class="col-lg-2 col-sm-2 col-4 col-status">
                                <span class="badge rounded-pill alert-success">Active</span>
                            </div>
                            <% const createdAt = "2023-05-16T05:58:23.405Z";
                            const date = new Date(i.createdAt);
                            const year = date.getFullYear().toString();
                            const month = (date.getMonth() + 1).toString().padStart(2, "0");
                            const day = date.getDate().toString().padStart(2, "0");
                            const convertedDate = `${day}.${month}.${year}`; %>
                            <div class="col-lg-1 col-sm-2 col-4 col-date">
                                <span><%= convertedDate  %></span>
                            </div>
                            <div class="col-lg-2 col-sm-2 col-4 col-action text-end">
                                <a href="/admin/product_edit/<%=i._id%>" class="btn btn-sm font-sm rounded btn-brand">
                                    <i class="material-icons md-edit"></i> Edit
                                </a>
                                <a onclick="setDeleteItem('<%= i._id %>')" class="btn btn-sm font-sm btn-light rounded">
                                    <!-- href="/admin/product_delete/<%=i._id%>" -->
                                    <i class="material-icons md-delete_forever"></i>  Delete
                                </a>
                            </div>
                        </div> <!-- row .// -->
