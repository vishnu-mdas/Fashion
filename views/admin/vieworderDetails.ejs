            <section class="content-main">
                <div class="content-header">
                  <div>
                    <h2 class="content-title card-title" style="font-family: Verdana, Geneva, Tahoma, sans-serif;">Order Details</h2>
                  </div>
                </div>
                <div class="card mb-4">
                  <div class="card-body">
                    <table class="table table-bordered">
                      <thead>
                        <tr class="main-heading">
                          <th>Image</th>
                          <th>Name</th>
                          <th>Description</th>
                          <th>Address</th>
                          <th>Date</th>
                          <th>Quantity</th>
                          <th>Selling Price</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% orderDetails.products.forEach(product => { %>  
                          <tr>
                            <td class="col-lg-1 col-sm-4 col-8">
                              <img src="/productImages/<%=product.productid.image[0] %>" class="img-sm img-thumbnail" alt="Item">
                            </td>
                            <td class="col-lg-2 col-sm-4 col-8">
                              <%= product.productid.productTitle %>
                            </td>
                            <td class="col-lg-2 col-sm-4 col-8">
                              <%= product.productid.description %>
                            </td>
                            <td class="col-lg-2 col-sm-4 col-8">
                              <%= orderDetails.deliveryAddress[0].fname %><%= orderDetails.deliveryAddress[0].lname %><br>
                              <%= orderDetails.deliveryAddress[0].billing_address %><br>
                              <%= orderDetails.deliveryAddress[0].state %>, <%= orderDetails.deliveryAddress[0].city %> <%= orderDetails.deliveryAddress[0].zipcode %><br>
                              <%= orderDetails.deliveryAddress[0].phone %>
                            </td>
                            <td class="col-lg-2 col-sm-4 col-8">
                              <%= orderDetails.date.toISOString().split('T')[0] %>
                            </td>
                            <td class="col-lg-1 col-sm-4 col-8">
                              <%= product.quantity %>
                            </td>
                            <td class="col-lg-1 col-sm-4 col-8">
                              â‚¹ <%= product.productid.offerPrice %>
                            </td>
                          </tr>
                        <% }) %>
                      </tbody>
                    </table>
              
                    <div class="row mt-3" style="justify-content: flex-end;">
                      <div class="col-md-10 h3">Total Amount:</div>
                      <div class="col-md-2 h4">â‚¹<%= orderDetails.totalAmount %></div>
                    </div>
                  </div>
                </div>
              </section>








