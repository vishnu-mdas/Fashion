            </div>
            <div class="card">
                <div class="card-body">
                    <div class="row">
                        <div class="col-lg-9">
                            <header class="border-bottom mb-4 pb-4">
                                <div class="row">
                                    
                                    
                                </div>
                            </header> <!-- card-header end// -->
                            <div class="table-responsive">
                                <table id="salesTable" class="table table-hover">
                                    <thead>
                                        <tr>
                                            <th>Order ID</th>
                                            <th>Delivery Address</th>
                                            <th>Date</th>
                                            <th>Amount</th>
                                            <th>Shipping Status</th>
                                            <th>Payment Method</th>
                                            <!-- <th class="text-end"> Action </th> -->
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% orders.forEach(order => { %>
                                            <tr>
                                              <td class="fw-bold"><%=order._id %></td>
                                              <td>
                                                <%= order.deliveryAddress[0].fname %> <%=
                                                order.deliveryAddress[0].lname%> <%=
                                                order.deliveryAddress[0].state%> <%=
                                                order.deliveryAddress[0].city%> <%=
                                                order.deliveryAddress[0].zipcode%>
                                              </td>
                                              <td><%= order.date.toISOString().split('T')[0] %></td>
                                              <td>&#8377;<%= order.totalAmount %></td>
                                              <td>
                                                <span class="badge badge-pill badge-soft-success"
                                                  ><%= order.shipping_status %></span>
                                              </td>
                                              <td><% if (order.paymentMethod === 'onlinepayment') { %>
                                                Online
                                              <% } else { %>
                                                COD
                                              <% } %></td>
                                            </tr>
                                            <% }) %>
                                        
                                    </tbody>
                                </table>
                            </div> <!-- table-responsive.// -->
